    <defName>ButcherFish</defName>
    <label>Butcher Fish</label>
    <description>Butcher Fish.</description>
    <jobString>Butchering.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>ButcherFlesh</effectWorking>
    <soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
    <workAmount>450</workAmount>
    <recipeUsers>
      <li>ButcherSpot</li>
      <li>TableButcher</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>DeadFish</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>DeadFish</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <TrimmedFish>1</TrimmedFish>
    </products>
    <skillRequirements>
      <Cooking>1</Cooking>
    </skillRequirements>
    <workSkill>Cooking</workSkill>
    <researchPrerequisite>StartFishing</researchPrerequisite>
  </RecipeDef>
